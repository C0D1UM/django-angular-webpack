    "font-awesome": "4.6.3",
    "jquery": "3.1.0",
    "lodash": "4.13.1"
  },
  "devDependencies": {
    "angular-mocks": "1.5.7",
    "babel-core": "6.10.4",
    "babel-istanbul": "0.11.0",
    "browser-sync": "2.13.0",
    "browser-sync-webpack-plugin": "1.1.0",
    "compression-webpack-plugin": "0.3.1",
    "css-loader": "0.23.1",
    "file-loader": "0.9.0",
    "html-webpack-plugin": "2.22.0",
    "istanbul": "0.4.4",
    "istanbul-instrumenter-loader": "0.2.0",
    "node-sass": "3.8.0",
    "raw-loader": "0.5.1",
    "sass-loader": "4.0.0",
    "style-loader": "0.13.1",
    "ts-loader": "0.8.2",
    "typescript": "1.8.10",
    "typings": "1.3.1",
    "url-loader": "0.5.7",
    "webpack": "1.13.1",
